                <role>developer</role>
            </roles>
        </developer>
        <developer>
            <id>maellis</id>
            <name>Mark Ellis</name>
            <email>maellis@adobe.com</email>
            <organization>adobeAtadobe</organization>
            <timezone>+5</timezone>
            <roles>
                <role>architect</role>
